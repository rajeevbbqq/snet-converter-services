{
  "name": "snet-converter-services",
  "version": "1.0.0",
  "description": "SNET Converter Services",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "serverless offline",
    "deploy": "serverless deploy",
    "test": "cross-env NODE_ENV=dev mocha -r ts-node/register tests/**/*.test.ts --exit",
    "coverage": "nyc --reporter lcov npm run test",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npm run typeorm -- migration:generate -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:create": "npm run typeorm -- migration:create -n"
  },
  "dependencies": {
    "aws-sdk": "^2.1003.0",
    "axios": "^0.21.4",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nanoid": "^3.1.29",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/chai": "^4.2.22",
    "@types/dotenv-safe": "^8.1.0",
    "@types/lambda-tester": "^3.6.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^15.14.9",
    "@types/sinon": "^9.0.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "faker": "^5.5.3",
    "husky": "^6.0.0",
    "istanbul": "^0.4.5",
    "lambda-tester": "^4.0.1",
    "lint-staged": "^11.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.4.1",
    "prettier-eslint": "^12.0.0",
    "serverless-dotenv-plugin": "^3.10.0",
    "serverless-layers": "^2.5.1",
    "serverless-offline": "^7.0.0",
    "serverless-plugin-split-stacks": "^1.11.2",
    "serverless-plugin-typescript": "^1.2.0",
    "serverless-prune-plugin": "^1.6.1",
    "sinon": "^9.0.2",
    "ts-node": "^8.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --relative"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
